pipeline {
    agent any
    tools {
        jdk 'java-17'        
        maven 'Maven 3.6.3'   
    }
    stages {
        stage('GIT') {
            steps {
                git branch: 'master', url: 'https://github.com/ademnsir/AndroidApp-AdemNsir4SIM1.git'
            }
        }
        stage('Compile Stage') {
            steps {
                sh 'mvn clean compile'
            }
        }
    }
}
