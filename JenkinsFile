pipeline {
    agent any
    tools {
        jdk 'JAVA_HOME'     
        maven 'Maven'       
    }
    stages {
        stage('GIT') {
            steps {
             
                git branch: 'master', url: 'https://github.com/ademnsir/AndroidApp-AdemNsir4SIM1.git'
            }
        }
        stage('Compile Stage') {
            steps {
             
                sh 'mvn clean compile'
            }
        }
    }
}
